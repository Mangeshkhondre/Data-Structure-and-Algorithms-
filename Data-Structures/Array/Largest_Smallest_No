class Largest_Smallest_No
{
	public static void main(String args[])
	{
		int a[]=new int[] {1,2,3,4,5,6};
		int max=0,min=0;
		for(int i=0;i<a.length;i++)
		{
			min=a[0];
			max=a[0];
			if(min>a[i])
			{
				min=a[i];
			}
			if(max<a[i])
			{
				max=a[i];
			}
		}
		System.out.println("Largest No = "+max+"\nSmallest No = "+min);
	}
}
